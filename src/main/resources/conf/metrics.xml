<stats>
    <stat name="networkInterfaceMetrics">
        <metric attr="RX KB" alias="RX KB" multiplier="9.59e-4"/>
        <metric attr="RX Dropped" alias="RX Dropped"/>
        <metric attr="RX Errors" alias="RX Errros"/>
        <metric attr="RX Overruns" alias="RX Overruns"/>
        <metric attr="RX Frame" alias="RX Frame"/>
        <metric attr="RX Packets" alias="RX Packets"/>
        <metric attr="Speed" alias="Speed"/>
        <metric attr="TX KB" alias="TX KB" multiplier="9.59e-4"/>
        <metric attr="TX Carrier" alias="TX Carrier"/>
        <metric attr="TX Collision" alias="TX Collision"/>
        <metric attr="TX Dropped" alias="TX Dropped"/>
        <metric attr="TX Errors" alias="TX Errors"/>
        <metric attr="TX Overruns" alias="TX Overruns"/>
        <metric attr="TX Packets" alias="TX Packets"/>
        <metric attr="TX Collision" alias="TX Collision"/>
    </stat>
    <stat name="TCP">
        <metric attr="Active Opens" alias="Active Opens"/>
        <metric attr="Attempt Fails" alias="Attempt Fails"/>
        <metric attr="Conn Established" alias="Conn Established"/>
        <metric attr="Resets Received" alias="Resets Received"/>
        <metric attr="Bad Segments" alias="Bad Segments"/>
        <metric attr="Resets Sent" alias="Resets Sent"/>
        <metric attr="Segments Received" alias="Segments Received"/>
        <metric attr="Segments Sent" alias="Segments Sent"/>
        <metric attr="Passive Opens" alias="Passive Opens"/>
        <metric attr="Segments Retransmitted" alias="Segments Retransmitted"/>
        <metric attr="Inbound Total" alias="Inbound Total"/>
        <metric attr="Outbound Total" alias="Outbound Total"/>
    </stat>
    <stat name="TCP|State">
        <metric attr="Bound" alias="Bound"/>
        <metric attr="Closed" alias="Closed"/>
        <metric attr="Close Wait" alias="Close Wait"/>
        <metric attr="Closing" alias="Closing"/>
        <metric attr="Establised" alias="Establised"/>
        <metric attr="Fin Wait1" alias="Fin Wait1"/>
        <metric attr="Fin Wait2" alias="Fin Wait2"/>
        <metric attr="Idle" alias="Idle"/>
        <metric attr="Last Ack" alias="Last Ack"/>
        <metric attr="Listen" alias="Listen"/>
        <metric attr="Syn Recv" alias="Syn Recv"/>
        <metric attr="Syn Sent" alias="Syn Sent"/>
        <metric attr="Time Wait" alias="Time Wait"/>
    </stat>
    <stat>
        <metric attr="All Inbound Total" alias="All Inbound Total"/>
        <metric attr="All Outbound Total" alias="All Outbound Total"/>
    </stat>
</stats>
